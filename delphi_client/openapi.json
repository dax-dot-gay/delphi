{
  "openapi": "3.0.0",
  "info": {
    "title": "delphi_api_lib",
    "version": "0.1.0"
  },
  "paths": {
    "/": {
      "get": {
        "tags": [
          "Root"
        ],
        "operationId": "get_status",
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/ApiStatus"
                }
              }
            }
          }
        }
      }
    },
    "/login": {
      "post": {
        "tags": [
          "Root"
        ],
        "operationId": "login",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/LoginModel"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/UserProfile"
                }
              }
            }
          },
          "500": {
            "description": "# 500 Internal Server Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/InternalServerError"
                }
              }
            }
          },
          "401": {
            "description": "# 401 Unauthorized",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Unauthorized"
                }
              }
            }
          },
          "405": {
            "description": "# 405 Method Not Allowed",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/MethodNotAllowed"
                }
              }
            }
          },
          "404": {
            "description": "# 404 Not Found",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/NotFound"
                }
              }
            }
          }
        }
      },
      "delete": {
        "tags": [
          "Root"
        ],
        "operationId": "logout",
        "responses": {
          "200": {
            "description": ""
          },
          "500": {
            "description": "# 500 Internal Server Error",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/InternalServerError"
                }
              }
            }
          },
          "401": {
            "description": "# 401 Unauthorized",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Unauthorized"
                }
              }
            }
          },
          "405": {
            "description": "# 405 Method Not Allowed",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/MethodNotAllowed"
                }
              }
            }
          },
          "404": {
            "description": "# 404 Not Found",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/NotFound"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "ApiStatus": {
        "type": "object",
        "required": [
          "session",
          "time"
        ],
        "properties": {
          "session": {
            "$ref": "#/components/schemas/Session"
          },
          "user": {
            "allOf": [
              {
                "$ref": "#/components/schemas/UserProfile"
              }
            ],
            "nullable": true
          },
          "time": {
            "type": "string",
            "format": "date-time"
          }
        }
      },
      "Session": {
        "type": "object",
        "properties": {
          "user_id": {
            "default": null,
            "type": "string",
            "nullable": true
          },
          "_docid": {
            "default": "4dd55985-45bf-491b-8dc6-c6c4ed7beb35",
            "type": "string"
          },
          "created": {
            "default": "2025-07-18T15:48:23.492916622Z",
            "type": "string",
            "format": "date-time"
          },
          "last_access": {
            "default": "2025-07-18T15:48:23.492926802Z",
            "type": "string",
            "format": "date-time"
          }
        }
      },
      "UserProfile": {
        "type": "object",
        "required": [
          "id",
          "is_admin",
          "username"
        ],
        "properties": {
          "id": {
            "type": "string"
          },
          "username": {
            "type": "string"
          },
          "is_admin": {
            "type": "boolean"
          }
        }
      },
      "InternalServerError": {
        "oneOf": [
          {
            "type": "object",
            "required": [
              "code",
              "reason"
            ],
            "properties": {
              "code": {
                "type": "string",
                "enum": [
                  "internal"
                ]
              },
              "reason": {
                "type": "string"
              }
            }
          }
        ]
      },
      "Unauthorized": {
        "oneOf": [
          {
            "type": "object",
            "required": [
              "code",
              "path"
            ],
            "properties": {
              "code": {
                "type": "string",
                "enum": [
                  "expects_authenticated"
                ]
              },
              "path": {
                "type": "string"
              }
            }
          }
        ]
      },
      "MethodNotAllowed": {
        "oneOf": [
          {
            "type": "object",
            "required": [
              "code",
              "id"
            ],
            "properties": {
              "code": {
                "type": "string",
                "enum": [
                  "logged_in"
                ]
              },
              "id": {
                "type": "string"
              }
            }
          }
        ]
      },
      "NotFound": {
        "oneOf": [
          {
            "type": "object",
            "required": [
              "code",
              "user"
            ],
            "properties": {
              "code": {
                "type": "string",
                "enum": [
                  "invalid_login"
                ]
              },
              "user": {
                "type": "string"
              }
            }
          }
        ]
      },
      "LoginModel": {
        "type": "object",
        "required": [
          "password",
          "username"
        ],
        "properties": {
          "username": {
            "type": "string"
          },
          "password": {
            "type": "string"
          }
        }
      }
    }
  }
}